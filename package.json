  "scripts": {
    "build": "babel src -d lib --source-maps inline",
    "merge": "NODE_ENV=production browserify -g uglifyify ./lib/index.js -o dist/index.js -i jquery && node node_modules/qili-app/scripts/allin1.js",
    "merge.debug": "browserify -d ./lib/index.js -o dist/test.js -i jquery",
    "merge.build": "watchify --ignore-watch -v -g uglifyify ./lib/index.js -o dist/index.js -i jquery",
    "karma": "karma start",
    "test": "restmock",
    "test.mock": "watchify -v -d ./.test.mock.js -o dist/test.js -i jquery",
    "test.mongo": "watchify --ignore-watch -v -d ./.test.mongo.js -o dist/test.js -i jquery",
    "cordova": "cordova create cordova --link-to=dist && cd cordova && cordova platform add android && cordova build android"
